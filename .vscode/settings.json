{
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "doxdocgen.generic.authorName": "Cheon Younghoe",
    "cmake.configureOnOpen": false,
    //"astyle.astylerc": '${workspaceRoot}/.vscode/astylerc',
    //"C_Cpp.formatting": "disabled",
    "python.poetryPath": "/bin/python3",
    "python.analysis.extraPaths": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/input",
        "${workspaceFolder}/src/log",
        "${workspaceFolder}/src/netlist",
        "${workspaceFolder}/src/run",
        "${workspaceFolder}/src/run_findvnet",
        "${workspaceFolder}/src/run_makeiprobe",
        "${workspaceFolder}/src/run_parser",
        "${workspaceFolder}/src/version",
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "black-formatter.args": [
        "--line-length",
        "119"
    ],
}